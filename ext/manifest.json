{
	"name": "mondora-door-opener",
	"manifest_version": 2,
	"description": "Open the door",
	"version": "1.0.0",
	"background": {
		"scripts": [
			"js/background.js"
		],
		"persistent": false
	},
	"permissions": [
		"activeTab",
		"tabs",
		"notifications"
	],
	"browser_action": {
		"default_icon": "img/icon.png"
	},
	"icons": {
		"128": "img/icon.png"
	}
}
